<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- XML declaration specifying version, encoding, and standalone attribute -->
<!DOCTYPE  Venue SYSTEM "Venue.dtd">
<!-- Document Type Definition (DTD) declaration -->
<Venue>
    <!-- Root element, representing a collection of venues -->
    <venue id="1">
        <!-- Start of the first venue entry with an ID attribute -->
        <name>Aviva Stadium</name>
        <!-- Name of the venue -->
        <address>62 Lansdowne Road, Dublin 4, Ireland</address>
        <!-- Address of the venue -->
        <capacity>51000</capacity>
        <!-- Maximum capacity of the venue -->
    </venue>
    <!-- End of the first venue entry -->
    <!-- ... (similar blocks for the remaining venues) ... -->
    <venue id="2">
        <name>Croke Park</name>
        <address>Jones' Rd, Drumcondra, Dublin, Ireland</address>
        <capacity>82700</capacity>
    </venue>
    <venue id="3">
        <name>Thomond Park</name>
        <address>Cratloe Rd, Limerick, Ireland</address>
        <capacity>25500</capacity>
    </venue>
    <venue id="4">
        <name>Pairc Ui Chaoimh</name>
        <address>Trabeg Sports Centre, Cork, Ireland</address>
        <capacity>45000</capacity>
    </venue>
</Venue>
<!-- End of the root element -->